
@{
    ViewBag.Title = "Index";
}

<html>
<body>
    <center><h1>Employee List</h1></center>
    <br /><br />
    <form>
        <div class="container">
            <table class="table">
                <thread>
                    <tr>
                        <th>Employee</th>
                        <th>Manager</th>
                    </tr>
                </thread>
                <tbody>
                    <tr>
                        <td>Hazel</td>
                        <td>Sample 1</td>
                    </tr>
                    <tr>
                        <td>Diana</td>
                        <td>Sample 2</td>
                    </tr>
                    <tr>
                        <td>Angelo</td>
                        <td>Sample 3</td>
                    </tr>
                    <tr>
                        <td>Christian</td>
                        <td>Sample 4</td>
                    </tr>
                    <tr>
                        <td>Khristel</td>
                        <td>Sample 5</td>
                    </tr>
                    <tr>
                        <td>Vince</td>
                        <td>Sample 6</td>
                    </tr>
                </tbody>
            </table>
            <!--Trigger the modal with a button-->
            <div class="modal-footer">
                <a href="https://www.skype.com/en/" class="btn btn-info" id="myButton" role="button" ng-click="model.create() " style="margin-right: 50px;float: right">Send</a>
                <a href="/Employee/Create" class="btn btn-info" id="myButton" role="button" ng- onclick="model.Create()" style="margin-right: 50px;float: right">Back</a>
                
                <!--Modal-->
                <div id="myModal" class="modal fade" role="dialog">
                    <div class="modal-dialog">

                        <!--Modal Content-->
                        <div class="modal-content">
                            <button type="button" class="close" data- dismiss="modal">&times;</button>
                            <div class="modal-body">
                                <p>Are you sure Do you want to save</p>
                                <div class="modal-footer">
                                    <button type="button" class=" btn btn-default" data- dismiss="modal">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</body>
</html>
